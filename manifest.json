{
  "manifest_version": 3,
  "name": "Pomodoro Timer",
  "version": "1.0.0",
  "description": "Study Pomodoro Timer",
  "permissions": ["alarms", "storage", "notifications"],
  "icons": {
    "16": "./icon.png",
    "32": "./icon.png",
    "48": "./icon.png",
    "128": "./icon.png"
  },
  "action": {
    "default_icon": {
      "16": "./icon.png",
      "24": "./icon.png",
      "32": "./icon.png"
    },
    "default_popup": "./popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options/options.html"
}
